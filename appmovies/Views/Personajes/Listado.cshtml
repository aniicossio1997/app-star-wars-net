@model PersonajesListadoModel
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<div class="container">
<a class="btn btn-dark" asp-area="" asp-controller="Personajes" asp-action="Index">Nuevo + </a>

<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Nombre</th>
    </tr>
  </thead>
  <tbody>
   
            @foreach (var personaje in Model.Listado)
            { <tr>
                  <th scope="row">@personaje.Id</th>
                  <td>@personaje.Name</td>
                </tr>

            }
  </tbody>
</table>


</div>

